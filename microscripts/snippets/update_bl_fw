clear

MAC=0060A70C55F6
IPv6=fe80::260:a7ff:fe0c:55f6

fw=SLCv3v4_LM_v7_1i.bin # eigentlich SLCv3v4_LM_v7_1i_rc483.bin, aber der Filename ist dem tftp Server (SLC) in einigen Versionen zu lang
bl=bl_SLCv34v2_4.bin

smudpset -ex -d $MAC -o reset 2
sleep 6
tftp -p -l $fw $IPv6
sleep 10
smudpset -ex -d $MAC -o bootloaderupload 30
sleep 6
tftp -p -l $bl $IPv6
sleep 45
smudpget -ex -d $MAC -o FirmwareVersion
smudpget -ex -d $MAC -o BootloaderVersion
